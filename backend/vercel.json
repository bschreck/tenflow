{
  "buildCommand": "python3 -m pip install uv && python3 -m  uv pip install . --target /build",
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/main"
    }
   ],
   "functions": {
    "api/**/*.py": {
      "memory": 3009,
      "maxDuration": 300
    }
  }
}