{
  "installCommand": "python3 -m pip install uv && python3 -m uv sync",
  "buildCommand": "python3 -m uv run uvicorn tenflow.main:app --host 0.0.0.0 --port 8000",
  "devCommand": "python3 -m uv run uvicorn tenflow.main:app --reload --host 0.0.0.0 --port 8000",
  "outputDirectory": "src",
  "env": {
    "PYTHONPATH": "src"
  }
}